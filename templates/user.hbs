{{#extend "index"}}
  {{#content "content"}}
    <p>Ваш уникальный код:</p>
    <h3>{{user.emojies}}</h3>
    {{#if pokemons}}
      <hr>
      <b>Пойманные покемоны:</b>
      <ul>
        {{#each pokemons}}
          <li><a href="/p/{{url}}">{{name}}</a></li>
        {{/each}}
      </ul>
    {{/if}}
  {{/content}}
{{/extend}}
